{
    "version": 3,
    "builds": [
      {
        "src": "/portfolio/portfolio/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", 
          "runtime": "python3.11" 
        }
      },
      {
        "src": "build_files.sh", 
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/home" 
      }
    ]
  }
